  },
  "homepage": "https://github.com/YuriyPent/Cypress_V6_UI-Automation-Testing#readme",
  "devDependencies": {
    "cypress": "^9.0.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-xpath": "^1.6.2"
  }
}
